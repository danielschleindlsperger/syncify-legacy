{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "../../dist/out-tsc",
    "jsx": "react",
    "allowJs": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "lib": ["es2019", "dom"],
    "skipLibCheck": true,
    "typeRoots": ["../../node_modules/@types/", "./typings/"]
  },
  "include": ["**/*.ts", "**/*.tsx", "**/*.d.ts"]
}
