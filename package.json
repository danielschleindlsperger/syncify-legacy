    "@types/spotify-web-api-node": "^4.0.1",
    "@types/spotify-web-playback-sdk": "^0.1.7",
    "@types/styled-system": "^5.1.3",
    "@types/styled-system__css": "^5.0.4",
    "@types/uuid": "^3.4.6",
    "@typescript-eslint/eslint-plugin": "^2.5.0",
