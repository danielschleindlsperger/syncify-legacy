{
  "name": "syncify",
  "version": "0.0.1",
  "description": "Synchronized Spotify listening rooms",
  "main": "",
  "scripts": {
    "proxy": "node proxy.js",
    "install:api": "cd ./api && npm i",
    "install:frontend": "cd ./frontend && npm i",
    "install:all": "concurrently \"npm run install:api\" \"npm run install:frontend\" ",
    "start:api": "cd ./api && npm start",
    "start:frontend": "cd ./frontend && npm run serve",
    "start:all": "concurrently \"npm run start:api\" \"npm run start:frontend\" "
  },
  "author": "Daniel Schleindlsperger <daniel@schleindlsperger.de>",
  "engines": {
    "node": ">= 8"
  },
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^3.6.0",
    "http-proxy": "^1.17.0",
    "http-proxy-rules": "^1.1.3"
  }
}
