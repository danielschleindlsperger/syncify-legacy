{
  "name": "syncify",
  "version": "0.0.1",
  "description": "Synchronized Spotify listening rooms",
  "author": "Daniel Schleindlsperger <daniel@schleindlsperger.de>",
  "main": "",
  "scripts": {
    "codegen": "./node_modules/.bin/graphql-codegen --config codegen.yml",
    "build:api": "backpack build",
    "build:webapp": "parcel build src/webapp/index.html",
    "build:deploy": "webpack -p",
    "build": "concurrently \"npm run build:api\" \"npm run build:webapp\" \"npm run build:deploy\" ",
    "start:api": "NODE_ENV=development backpack",
    "start:webapp": "parcel src/webapp/index.html",
    "prestart": "serverless deploy --config infrastructure/serverless.yml --profile dev",
    "start": "node ./dev/start.js",
    "storybook": "start-storybook --quiet --ci -p 6006",
    "build-storybook": "build-storybook --quiet",
    "test": "jest",
    "tsc": "tsc",
    "lint": "eslint src/**/*.{ts,tsx}",
    "package-artifact": "mv node_modules build/node_modules && cd build && zip -r artifact.zip node_modules app.js app.map",
    "deploy": "node build/deploy.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "engines": {
    "node": "12"
  },
  "alias": {
    "react-dom": "@hot-loader/react-dom"
  },
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@babel/preset-typescript": "^7.7.0",
    "@emotion/babel-preset-css-prop": "^10.0.23",
