{
  "name": "syncify-api",
  "version": "0.0.0",
  "description": "Backend for Syncify",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest --expand --runInBand",
    "build": "backpack build",
    "start:dev": "NODE_ENV=development backpack",
    "start:prod": "NODE_ENV=production node build/main.js",
    "db:seed": "NODE_ENV=development ts-node src/seed/index.ts",
    "db:drop": "NODE_ENV=development ts-node src/seed/drop.ts",
    "docker:build": "docker build -t repo.treescale.com/liachthuaber/syncify-api:latest .",
    "docker:publish": "docker push repo.treescale.com/liachthuaber/syncify-api:latest",
    "deploy": "kubectl --kubeconfig=\"../k8s-syncify-kubeconfig.yaml\" apply -f ./deploy.yaml"
  },
  "author": "Daniel Schleindlsperger <daniel@schleindlsperger.de>",
  "license": "ISC",
  "dependencies": {
    "@marblejs/core": "^1.2.1",
    "@marblejs/middleware-body": "^1.2.1",
    "@marblejs/middleware-joi": "^1.2.1",
    "@marblejs/middleware-jwt": "^1.2.1",
    "@marblejs/middleware-logger": "^1.2.1",
    "axios": "^0.18.0",
    "chalk": "^2.4.1",
    "dotenv": "^6.2.0",
    "mysql": "^2.16.0",
    "rabbit-queue": "^3.0.2",
    "ramda": "^0.25.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "~6.2.2",
    "socket.io": "^2.2.0",
    "spotify-web-api-node": "^4.0.0",
    "ts-node": "^7.0.1",
    "type-env": "^1.1.0",
    "typeorm": "^0.2.11",
    "typescript": "^3.2.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/dotenv": "^4.0.3",
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.18",
    "@types/ramda": "^0.25.45",
    "@types/socket.io": "^2.1.2",
    "@types/supertest": "^2.0.7",
    "backpack-core": "^0.8.3",
    "faker": "^4.1.0",
    "jest": "^23.6.0",
    "mock-req": "^0.2.0",
    "supertest": "^3.3.0",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.2",
    "uuid": "^3.3.2"
  }
}
